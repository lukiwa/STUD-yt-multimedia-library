<Window x:Class="YtMultimediaLibrary.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YtMultimediaLibrary"
        mc:Ignorable="d"
        Title="LoginWindow" Height="412.622" Width="429.153">
    <Grid>
        <TextBox 
            x:Name="emailTextBox"
            HorizontalAlignment="Left" 
            Height="25" Margin="117,100,0,0" 
            TextWrapping="Wrap" Text="E-mail" 
            VerticalAlignment="Top" 
            Width="199"
            />
        <TextBox 
            x:Name="passwordTextBox"
            HorizontalAlignment="Left" 
            Height="24" Margin="117,164,0,0" 
            TextWrapping="Wrap" 
            Text="Password" 
            VerticalAlignment="Top" 
            Width="199"
            />
        <Button 
            Style="{StaticResource RoundButtonTemplate}"
            Click="SignIn_Click"
            Content="Sign In" 
            HorizontalAlignment="Left" 
            Margin="140,221,0,0" 
            VerticalAlignment="Top" 
            Width="137" 
            Height="47"/>

    </Grid>
</Window>
